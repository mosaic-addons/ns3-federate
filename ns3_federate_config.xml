    <!-- <default name="ns3::ConstantRateWifiManager::DataMode" value="OfdmRate6Mbps"/> -->
    <!-- <default name="ns3::ConstantRateWifiManager::ControlMode" value="OfdmRate6Mbps"/> -->

    <!-- <default name="ns3::WifiMacQueue::MaxDelay" value="+500000000.0ns"/> -->
